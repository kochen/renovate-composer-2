{
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "rangeStrategy": "pin",
  "composerIgnorePlatformReqs": false,
  "postUpgradeTasks": {
    "commands": [
      "[ -f any.file ] && echo 'hi'"
    ],
    "fileFilters": ["**/**"]
  },
}
