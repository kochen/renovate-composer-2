{
    "name": "webpower/wp20",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.9",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "api-platform/api-pack": "^1.3.0",
        "api-platform/core": "^2.5.7",
        "doctrine/dbal": "^2.12.0",
        "doctrine/doctrine-bundle": "^2.1.2",
        "doctrine/doctrine-migrations-bundle": "^3.0.1",
        "doctrine/migrations": "^3.0.1",
        "doctrine/orm": "^2.7.4",
        "doctrine/persistence": "^1.3.7 || ^2.0.0",
        "giggsey/libphonenumber-for-php": "^8.12.11",
        "league/flysystem": "^1.1.3",
        "league/flysystem-bundle": "^1.5.0",
        "lexik/jwt-authentication-bundle": "^2.9.0",
        "marc-mabe/php-enum": "^4.4.0",
        "maxakawizard/json-collection-parser": "^1.7.0",
        "peppeocchi/php-cron-scheduler": "^3.1",
        "prooph/common": "^4.4.0",
        "prooph/event-sourcing": "^5.6.0",
        "prooph/event-store": "^7.5.9",
        "prooph/pdo-event-store": "^1.12.0",
        "psr/container": "^1.0.0",
        "ramsey/uuid": "^4.1.1",
        "ramsey/uuid-doctrine": "^1.6.0",
        "symfony/amqp-messenger": "^5.1.7",
        "symfony/cache-contracts": "^2.2",
        "symfony/config": "^5.1.7",
        "symfony/console": "^5.1.7",
        "symfony/dependency-injection": "^5.1.7",
        "symfony/doctrine-messenger": "^5.1.7",
        "symfony/flex": "^1.9.10",
        "symfony/framework-bundle": "^5.1.7",
        "symfony/http-client": "^5.1.7",
        "symfony/http-client-contracts": "^2.3.1",
        "symfony/http-foundation": "^5.1.7",
        "symfony/http-kernel": "^5.1.7",
        "symfony/messenger": "^5.1.7",
        "symfony/mime": "^5.1.7",
        "symfony/process": "^5.1.7",
        "symfony/property-info": "^5.1.7",
        "symfony/routing": "^5.1.7",
        "symfony/security-core": "^5.1.7",
        "symfony/serializer": "^5.1.7",
        "symfony/validator": "^5.1.7",
        "symfony/yaml": "^5.1.7",
        "thecodingmachine/safe": "^1.3.2",
        "webmozart/assert": "^1.9.1"
    },
    "require-dev": {
        "behat/behat": "^3.7.0",
        "dama/doctrine-test-bundle": "^6.3.3",
        "doctrine/coding-standard": "^8.2.0",
        "maglnet/composer-require-checker": "^2.1.0",
        "phpunit/phpunit": "^9.4.2",
        "psalm/plugin-phpunit": "^0.13.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs-de/deptrac-shim": "^0.8.2",
        "symfony/browser-kit": "^5.1.7",
        "vimeo/psalm": "^4.0.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "ext-dom": "1.0",
            "ext-pdo_mysql": "1.0",
            "ext-simplexml": "1.0",
            "ext-xml": "1.0",
            "ext-xmlwriter": "1.0"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Achievement\\": "src/Achievement/",
            "Audience\\": "src/Audience/",
            "BatchImport\\": "src/BatchImport/",
            "Company\\": "src/Company/",
            "Contact\\": "src/Contact/",
            "Content\\": "src/Content/",
            "ContactField\\": "src/ContactField/",
            "ContactInteraction\\": "src/ContactInteraction/",
            "LeadScoreRule\\": "src/LeadScoreRule/",
            "License\\": "src/License/",
            "MailAudience\\": "src/MailAudience/",
            "Schedule\\": "src/Schedule/",
            "Scoring\\": "src/Scoring/",
            "Shared\\": "src/Shared/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Feature\\": "test/Feature",
            "Test\\Integration\\": "test/Integration",
            "Test\\Unit\\": "test/Unit"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
